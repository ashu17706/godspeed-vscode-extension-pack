{
  "name": "godspeed",
  "displayName": "Godspeed Extension Pack",
  "description": "VSCode extension for development assistance with Godspeed micro-service framework.",
  "publisher": "zero8",
  "license": "MIT",
  "version": "1.3.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "author": {
    "name": "Ashutosh Tripathi"
  },
  "homepage": "https://github.com/ashu17706/godspeed-vscode-extension-pack/main/README.md",
  "bugs": {
    "url": "https://github.com/ashu17706/godspeed-vscode-extension-pack/issues",
    "email": "hello@zero8.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashu17706/godspeed-vscode-extension-pack.git"
  },
  "categories": [
    "Snippets",
    "Linters",
    "Formatters",
    "Extension Packs",
    "Programming Languages"
  ],
  "keywords": [
    "godspeed",
    "microservice",
    "low-code",
    "no-code"
  ],
  "extensionDependencies": [
    "redhat.vscode-yaml",
    "Prisma.prisma"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "yamlValidation": {
      "fileMatch": "src/events/**/*.yaml",
      "url": "./schemas/events.json"
    },
    "snippets": [
      {
        "language": "yaml",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [
      {
        "command": "godspeed.install",
        "title": "Install Godspeed YAML's grammer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "ts-loader": "^9.4.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "@vscode/test-electron": "^2.2.0"
  }
}
